        "contentlayer2": "0.5.8",
        "gray-matter": "^4.0.3",
        "hastscript": "^9.0.1",
        "mdast-util-toc": "^7.1.0",
        "next": "15.2.1",
        "next-contentlayer2": "0.5.8",
        "primeicons": "catalog:",
        "primereact": "workspace:*",
        "react": "catalog:react19",
        "react-dom": "catalog:react19",
        "rehype-autolink-headings": "^7.1.0",
