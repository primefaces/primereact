{
    "name": "showcase",
    "version": "11.0.0",
    "scripts": {
        "dev": "npm run dev:init && next dev --turbopack",
        "dev:init": "npm run clean && npm run build:init",
        "scan": "npm run dev && react-scan localhost:3000",
        "build": "npm run build:init && next build",
        "build:init": "npm run build:store && npm run build:content",
        "build:content": "contentlayer2 build",
        "build:store": "node ./scripts/generate-store.mjs",
        "build:apidoc": "node ./scripts/build-apidoc.mjs",
        "clean": "npx rimraf --glob ./__store__ ./.contentlayer ./.next",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@primereact/code-viewer": "workspace:*",
