{
    "name": "@primereact/headless",
    "version": "11.0.0",
    "author": "PrimeTek Informatics",
    "description": "",
    "homepage": "https://primereact.org/",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/primefaces/primereact.git",
        "directory": "packages/headless"
    },
    "bugs": {
        "url": "https://github.com/primefaces/primereact/issues"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*/index.ts"
    },
    "publishConfig": {
        "main": "./index.mjs",
        "module": "./index.mjs",
        "types": "./index.d.ts",
        "exports": {
            ".": {
                "types": "./index.d.ts",
                "import": "./index.mjs",
                "default": "./index.mjs"
            },
            "./*": {
                "types": "./*/index.d.ts",
                "import": "./*/index.mjs",
                "default": "./*/index.mjs"
            }
        },
        "directory": "dist",
        "linkDirectory": false,
        "access": "public"
    },
    "scripts": {
        "build": "NODE_ENV=production INPUT_DIR=src/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && tsup && tsc -p tsconfig.build.json && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "type:check": "tsc --noEmit",
        "test": "vitest --config=../../vitest.config.ts",
        "test:ui": "vitest --config=../../vitest.config.ts --ui",
        "test:coverage": "vitest --config=../../vitest.config.ts --coverage",
        "dev:link": "pnpm link --global && npm link"
    },
    "dependencies": {
        "@primeuix/styled": "catalog:",
        "@primeuix/utils": "catalog:",
        "@primereact/core": "workspace:*",
        "@primereact/hooks": "workspace:*"
    }
}
